<Window x:Class="Xram_KR5.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Xram_KR5"
        mc:Ignorable="d"
        Title="MainWindow" Height="600" Width="1000">
    <Grid>
       
        <Grid.RowDefinitions>
            <RowDefinition Height="70"></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <Grid Grid.Row="0" Background="#FF76E383">
            <Label Content="ИС по прокату спортивного инвентаря" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="30"></Label>
            <Image Source="Res/logo.png" VerticalAlignment="Top" HorizontalAlignment="Left"></Image>
        </Grid>
        <Frame x:Name="MainFrame" Grid.Row="1" NavigationUIVisibility="Hidden"></Frame>
    </Grid>
  
</Window>
